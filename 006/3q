#import <stdio.h>
#import <math.h>

int sumSquareDifference(int amount);

int main() {
	printf("The difference between the sum of the squares of the first one hundred natural numbers and the square of the sum is %d\n", sumSquareDifference(100));
	return 0;
}

int sumSquareDifference(int amount) {
	int sum;
	int square;
	for (int i = 0; i < amount; i++) {
		sum += (int) pow(i + 1, 2);
		square += i + 1;
	}
	square = (int) pow(square, 2);
	return square - sum;
}
